{
  "name": "joburivue",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Andrei1524/joburivue.git",
  "scripts": {
    "install-client": "npm install --prefix client",
    "install-server": "npm install --prefix server",
    "deploy": "npm run build-prod --prefix client && npm run build --prefix server",
    "server": "npm run watch --prefix server",
    "client": "npm run dev --prefix client",
    "watch": "concurrently \"npm run server\" \"npm run client\"",
    "start": "concurrently \"node server/server-dist/server.js\" \"node server/.output/server/index.mjs\""
  },
  "author": "Mandrican Andrei <adyta094@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.0.0"
  }
}
