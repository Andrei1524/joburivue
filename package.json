{
  "name": "joburivue",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Andrei1524/joburivue.git",
  "scripts": {
    "install-client": "yarn install --prefix client",
    "install-server": "yarn install --prefix server",
    "deploy": "yarn run build-prod --prefix client && yarn start --prefix server",
    "server": "yarn run watch --prefix server",
    "client": "yarn run dev --prefix client",
    "watch": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "author": "Mandrican Andrei <adyta094@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.0.0"
  }
}
